<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- TrackpadView -->
    <declare-styleable name="TrackpadView" >
        <attr name="baseColor" format="color"/>
        <attr name="borderRadius" format="dimension"/>
    </declare-styleable>

    <!-- Knob -->
    <declare-styleable name="Knob" >
        <attr name="circleColor" format="color"/>
        <attr name="strokeColor" format="color"/>
        <attr name="strokeWidth" format="dimension"/>
        <attr name="notchColor" format="color"/>
        <attr name="notchDisplacement" format="float"/>
        <attr name="notchSizePerc" format="float"/>
    </declare-styleable>

</resources>